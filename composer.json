{
	"name": "filipsedivy/rajce-proxy",
	"description": "Proxy for API rajce.idnes.cz",
	"type": "project",
	"license": "MIT",
	"require": {
		"php": ">= 7.2",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.1",
		"nette/caching": "^3.1",
		"nette/di": "^3.0",
		"nette/finder": "^2.5",
		"nette/forms": "^3.1",
		"nette/http": "^3.1",
		"nette/mail": "^3.1",
		"nette/robot-loader": "^3.3",
		"nette/security": "^3.1",
		"nette/utils": "^3.2",
		"tracy/tracy": "^2.8",
		"apitte/core": "^0.8.1",
		"nette/schema": "^1.2",
		"fabpot/goutte": "^4.0",
		"apitte/debug": "^0.8.0",
		"apitte/negotiation": "^0.8.0",
		"latte/latte": "^2.10"
	},
	"require-dev": {
		"nette/tester": "^2.3",
		"symfony/thanks": "^1",
		"phpstan/phpstan": "^1.1",
		"nette/coding-standard": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"scripts": {
		"phpstan": "phpstan analyse app --level max",
		"check-cs": "ecs check app/ bin/ config/ tests/ www/ --config tests/coding-standards.php",
		"fix-cs": "ecs check app/ bin/ config/ tests/ www/ --config tests/coding-standards.php --fix"
	}
}
